{
    "manifest_version": 3,
    "name": "My React Extension",
    "version": "1.0",
    "description": "A simple web extension built with React",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "permissions": ["storage"],
    "host_permissions": ["*://*/*"]
  }